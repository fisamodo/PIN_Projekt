@model IEnumerable<PIN_Projekt.Models.Parking>

@{
    ViewData["Title"] = "Detaljni prikaz";
}
@foreach (var item in Model)
    {

     

    <dl class="row">
        <dt class="col-sm-2">
            Registracijska oznaka vozila
        </dt>
        <dd class="col-sm-10">
            @item.registracija
        </dd>
        <dt class="col-sm-2">
            Model vozila
        </dt>
        <dd class="col-sm-10">
            @item.model
        </dd>
        <dt class="col-sm-2">
            Broj osoba u vozilu
        </dt>
        <dd class="col-sm-10">
            @item.brojOsoba
        </dd>
    </dl>
    <a asp-action="Index">Povratak</a>


}



